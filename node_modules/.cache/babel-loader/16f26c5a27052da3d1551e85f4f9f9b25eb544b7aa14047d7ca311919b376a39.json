template>\r\n    </el-table-column>\r\n  </el-table>\r\n    </div>\r\n</template>\r\n<style>\r\n    .el-table{\r\n        text-align:center\r\n    }\r\n    .el-table-column.label{\r\n        text-align:center\r\n    }\r\n</style>\r\n\r\n<script>\r\n  import axios from 'axios'\r\n  import global from './GlobalPage.vue'\r\n  const restweburl = global.ip;\r\n  export default {\r\n    created() {\r\n      axios.get(restweburl + \"getBranch\", {params:{'wName':this.$parent.$route.params.id}})\r\n        .then((res) => {\r\n          console.log(res.data.data);\r\n          this.options=res.data.data;\r\n        })\r\n        .catch(function (error) {\r\n          console.log(error);\r\n        });\r\n        this.loading=true;\r\n        axios.get(restweburl + \"getFiles\", {params:{'wName':this.$parent.$route.params.id,'path':'','branch':'master'}})\r\n        .then((res) => {\r\n          this.tableData=res.data.data;\r\n          this.loading=false;\r\n        })\r\n        .catch(function (error) {\r\n          console.log(error);\r\n        });\r\n    },\r\n    computed(){\r\n      \r\n    },\r\n    methods: {\r\n      change(){\r\n        this.loading=true;\r\n        axios.get(restweburl + \"getFiles\", {params:{'wName':this.$parent.$route.params.id,'path':'','branch':this.$refs.myselect.selected.value}})\r\n        .then((res) => {\r\n          this.tableData=res.data.data;\r\n          this.loading=false;\r\n        })\r\n        .catch(function (error) {\r\n          console.log(error);\r\n        });\r\n      },\r\n      startRow(index, rows) {\r\n        this.loading=true;\r\n        console.log(rows);\r\n        axios.get(restweburl + \"getFiles\", {params:{'wName':this.$parent.$route.params.id,'path':rows.path+rows.fileName,'branch':'master'}})\r\n        .then((res) => {\r\n          this.tableData=res.data.data;\r\n          this.loading=false;\r\n        })\r\n        .catch(function (error) {\r\n          console.log(error);\r\n        });\r\n      },\r\n    },\r\n    data() {\r\n      return {\r\n        value:'',\r\n        loading:true,\r\n        now:'',\r\n        tableData: [\r\n          {\r\n          type:'<i class=\"el-icon-folder\"></i>',\r\n          name:'image',\r\n          description:'原神？启动！',\r\n          date:'last month'\r\n        },\r\n          ],\r\n          options: ['原神？启动！','Genshin Impact'],\r\n      }\r\n    }\r\n  }\r\n</script>"],"mappings":"AAuEA,OAAAA,KAAA;AACA,OAAAC,MAAA;AACA,MAAAC,UAAA,GAAAD,MAAA,CAAAE,EAAA;AACA;EACAC,QAAA;IACAJ,KAAA,CAAAK,GAAA,CAAAH,UAAA;MAAAI,MAAA;QAAA,cAAAC,OAAA,CAAAC,MAAA,CAAAF,MAAA,CAAAG;MAAA;IAAA,GACAC,IAAA,CAAAC,GAAA;MACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA,CAAAG,IAAA,CAAAA,IAAA;MACA,KAAAC,OAAA,GAAAJ,GAAA,CAAAG,IAAA,CAAAA,IAAA;IACA,GACAE,KAAA,WAAAC,KAAA;MACAL,OAAA,CAAAC,GAAA,CAAAI,KAAA;IACA;IACA,KAAAC,OAAA;IACAlB,KAAA,CAAAK,GAAA,CAAAH,UAAA;MAAAI,MAAA;QAAA,cAAAC,OAAA,CAAAC,MAAA,CAAAF,MAAA,CAAAG,EAAA;QAAA;QAAA;MAAA;IAAA,GACAC,IAAA,CAAAC,GAAA;MACA,KAAAQ,SAAA,GAAAR,GAAA,CAAAG,IAAA,CAAAA,IAAA;MACA,KAAAI,OAAA;IACA,GACAF,KAAA,WAAAC,KAAA;MACAL,OAAA,CAAAC,GAAA,CAAAI,KAAA;IACA;EACA;EACAG,SAAA,GAEA;EACAC,OAAA;IACAC,OAAA;MACA,KAAAJ,OAAA;MACAlB,KAAA,CAAAK,GAAA,CAAAH,UAAA;QAAAI,MAAA;UAAA,cAAAC,OAAA,CAAAC,MAAA,CAAAF,MAAA,CAAAG,EAAA;UAAA;UAAA,eAAAc,KAAA,CAAAC,QAAA,CAAAC,QAAA,CAAAC;QAAA;MAAA,GACAhB,IAAA,CAAAC,GAAA;QACA,KAAAQ,SAAA,GAAAR,GAAA,CAAAG,IAAA,CAAAA,IAAA;QACA,KAAAI,OAAA;MACA,GACAF,KAAA,WAAAC,KAAA;QACAL,OAAA,CAAAC,GAAA,CAAAI,KAAA;MACA;IACA;IACAU,SAAAC,KAAA,EAAAC,IAAA;MACA,KAAAX,OAAA;MACAN,OAAA,CAAAC,GAAA,CAAAgB,IAAA;MACA7B,KAAA,CAAAK,GAAA,CAAAH,UAAA;QAAAI,MAAA;UAAA,cAAAC,OAAA,CAAAC,MAAA,CAAAF,MAAA,CAAAG,EAAA;UAAA,QAAAoB,IAAA,CAAAC,IAAA,GAAAD,IAAA,CAAAE,QAAA;UAAA;QAAA;MAAA,GACArB,IAAA,CAAAC,GAAA;QACA,KAAAQ,SAAA,GAAAR,GAAA,CAAAG,IAAA,CAAAA,IAAA;QACA,KAAAI,OAAA;MACA,GACAF,KAAA,WAAAC,KAAA;QACAL,OAAA,CAAAC,GAAA,CAAAI,KAAA;MACA;IACA;EACA;EACAH,KAAA;IACA;MACAY,KAAA;MACAR,OAAA;MACAc,GAAA;MACAb,SAAA,GACA;QACAc,IAAA;QACAC,IAAA;QACAC,WAAA;QACAC,IAAA;MACA,EACA;MACArB,OAAA;IACA;EACA;AA